import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int gcd=1;
        int lcm=1;
        int a=sc.nextInt();
        int may=(a>n) ? a:n;
        for(int i=1;i<=may;i++){
            if(n%i==0 && a%i==0)
            gcd=i;
        }
     System.out.println(gcd);
    }
}